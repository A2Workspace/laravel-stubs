{
    "name": "a2workspace/laravel-stubs",
    "description": "一個基於專案的類別模板注入器。",
    "license": "MIT",
    "authors": [
        {
            "name": "Shishamou",
            "email": "shishatw225@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "6.x"
    },
    "autoload": {
        "psr-4": {
            "A2Workspace\\Stubs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "A2Workspace\\Stubs\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ],
        "test-coverage": [
            "vendor/bin/phpunit --coverage-html coverage"
        ]
    }
}
